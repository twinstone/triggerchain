{
  "name": "triggerchain",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --sourcemap --format cjs,esm --dts --clean",
    "tsc": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "tsup": "^6.2.3",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "@types/react": "^18.0.18",
    "react": "^18.2.0"
  }
}